{
  "name": "@example/root",
  "version": "0.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "@example/root",
      "version": "0.0.0",
      "workspaces": [
        "packages/greet",
        "packages/app"
      ]
    },
    "node_modules/@example/app": {
      "resolved": "packages/app",
      "link": true
    },
    "node_modules/@example/greet": {
      "resolved": "packages/greet",
      "link": true
    },
    "packages/app": {
      "name": "@example/app",
      "version": "0.0.0",
      "dependencies": {
        "@example/greet": "^0.0.0"
      }
    },
    "packages/greet": {
      "name": "@example/greet",
      "version": "0.0.0"
    }
  },
  "dependencies": {
    "@example/app": {
      "version": "file:packages/app",
      "requires": {
        "@example/greet": "*"
      }
    },
    "@example/greet": {
      "version": "file:packages/greet"
    }
  }
}
